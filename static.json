{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/index.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0",
      "Strict-Transport-Security": "max-age=7776000"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}