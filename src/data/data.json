    "design": [
        {
            "name": "Washed Away",
            "url": "/design/washed-away",
            "thumbPath": "../images/design/washed-away-thumb.jpg",
            "subContent": {
                "images": [
                    "../images/design/washed-away-1.jpg",
                    "../images/design/washed-away-2.jpg",
                    "../images/design/washed-away-3.jpg",
                    "../images/design/washed-away-4.jpg"
                ],
                "videoLink": "../videos/washed_away_small",
                "desc": "Washed away was a typography experiment where a Garamond letter \"A\" was sculpted out of sand and videoed while slowly getting washed away by the tide. Images from the video were published in Barbara Brownies book Typed Image. It was a really fun project but tense as well. Contending with the impeding tide made it quite difficult to time properly. This video is embedded locally, using the HTML5 video element."
            }
        },
        {
            "name": "More Than Design",
            "url": "/design/more-than-design",
            "thumbPath": "../images/design/more-than-design-thumb.jpg",
            "subContent": {
                "images": [
                    "../images/design/more-than-design-show-1.jpg",
                    "../images/design/more-than-design-show-2.jpg",
                    "../images/design/more-than-design-show-3.jpg",
                    "../images/design/more-than-design-show-4.jpg"
                ],
                "desc": "‘More Than Design’ was a gallery for our senior thesis projects. My project was the creation of www.actkindblog.com, a community blog about acts of kindness. Users are able to write and comment about kind acts they have done, seen, or heard about. To accompany the site, a physical blog page was printed and displayed in the show. People were encouraged to write kind acts on post-it notes and stick them on the poster and surrounding walls. 150 kind act post-it notes were put up that night. These post-its were then photographed and are periodically posted on the blog under the title ‘150 Days of Kindness.’"
            }
        },
        {
            "name": "Peoples Inc.",
            "url": "/design/peoples-inc",
            "thumbPath": "../images/design/people-inc-thumb.jpg",
            "subContent": {
                "images": [
                    "../images/design/people-inc-1.jpg",
                    "../images/design/people-inc-2.jpg"
                ],
                "desc": "People Incorporated tasked us with re-designing and re-evaluating their space for improved functionality. A re-organization system was created to achieve the utmost service to their customers. We displayed our ideas and suggestion to them through a scaled model of their first floor. Some of the recommendations we proposed were, better lighting, more efficient storage and lighter wall colors."
            }
        },
