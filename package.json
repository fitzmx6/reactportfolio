    "react-scripts": "5.0.1",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "sass": "^1.92.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
