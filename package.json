{
  "name": "react-portfolio",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.10.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "sass src/styles/style.scss src/styles/style.css",
    "watch-css": "npm run build-css && sass --watch src/styles/style.scss src/styles/style.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "sass": "^1.87.0"
  }
}
